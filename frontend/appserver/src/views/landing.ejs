    /* -------------------------------------------------------------------------- */
    #sec1 {
      max-width: 1400px;
      margin: 0 auto;
      padding: 124px 0px 0px 32px;
    }
    #sec1-left {
      float: left;
      width: 50%;
    }
    #sec1-intro {
      padding: 100px 0px 100px 10px;
      max-width: 620px;
    }
    .sec1-login {
      margin-top: 20px;
    }
    .sec1-login a {
      display: inline-block;
      margin-right: 3px; padding: 10px 17px;
