    /* -------------------------------------------------------------------------- */
      #sec5 {
      text-align: center;
        max-width: 1400px;
        padding: 100px 0px 0px 32px;
        margin: 0 auto;
    }
    #quote {
        padding: 0px 70px 0px;
        background-size: 53px 51px !important;
    }
    #quote p {
      font-style: italic;
        padding-bottom:80px;
    }
    /* -------------------------------------------------------------------------- */
      #faq {
        max-width: 770px;
        padding: 0px 0px 100px 32px;
        margin: 0 auto;
    }
      #faq h2 {
      font-size: 1.5em;
        font-family: '72bold', BentonSans, arial, sans-serif;
        padding-top:50px;
    }
      #faq dl {
        width: 100%;
    }
      #faq dt {
      margin: 48px 0px 8px;
      font-size: 1.25em;
        font-family: '72regular', BentonSans, arial, sans-serif;
      }
      #faq p {
        margin: 16px 0px 8px;
        font-size: 1em;
        line-height: 1.8;
        font-weight: 100;
        color: #6A6D70;
    }
      #faq li p {
        margin: 0px;
      }
      #faq dt, dd {
      line-height: 2em;
    }
      #faq-contact-us {
      margin-top: 60px;
      text-align: center;
    }
      #faq-contact-us h3 {
        font-family: '72regular', BentonSans, arial, sans-serif;
      font-size: 1.7em;
        font-weight: normal;
    }
      #faq-contact-us p {
      margin-top: 0;
      font-size: 1.25em;
    }
      a#mail-button {
      padding: 10px 22px;
      border: solid 1px #0A6ED1; border-radius: 4px;
      box-shadow: 0px 1px 0px 0px rgba(0, 0, 0, 0.06);
      text-decoration: none;
      font-size: 0.7em;
      color: #0A6ED1;
    }
      a#mail-button:hover {
      border: solid 2px #0A6ED1; border-radius: 4px;
      text-decoration: none;
    }
    /* -------------------------------------------------------------------------- */
      footer {
      clear: both;
        margin: 0px auto; padding: 32px 32px 50px;
      max-width: 1400px;
      font-size: 0.875em;
        background-color: #fff;
    }
    #footer-tagline {
      float: left;
      padding-right: 60px;
      height: 24px;
      background-size: 48px 24px !important;
      line-height: 24px;
      font-family: '72bold', BentonSans, arial, sans-serif;
    }
    #footer-nav {
      float: right;
      text-align: right;
    }
    #footer-nav a {
      text-decoration: none;
      color: #6A6D70;
    }
    #footer-nav a:hover {
      text-decoration: underline;
    }
    #footer-nav ul {
      list-style: none;
    }
    #footer-nav li {
      float: left;
      display: inline-block;
      margin-left: 25px;
    }
    /* ========================================================================== */
    @media only screen and (max-width : 840px) {
      #sec1-intro {
        padding: 40px 0px 40px 10px;
      }
        #sec3 {
          padding: 40px 32px;
      }
        #floating-boxes {
        margin: 40px auto 0px;
      }
        #floating-boxes .box {
          margin: 0px 0px 60px;
          padding: 20px 10px 0px;
      }
        #sec4-right {
        padding: 40px 10px 40px 60px;
      }
        #sec5 {
          padding: 40px 10px 0px;
      }
        #sec6 {
          padding: 0px 32px 40px 32px;
      }
        #sec6 dl {
        width: 100%;
      }
      #footer-nav {
        clear: both;
        padding: 30px 0px;
      }
      :target:before {
        content: "";
        display: block;
        height: 64px; /* fixed header height*/
        margin: -64px 0 0; /* negative fixed header height */
      }
    }
    /* -------------------------------------------------------------------------- */
    @media only screen and (max-width: 640px) {
        body {
          font-size: 100%;
        }
        h1 {
          font-size: 2em;
        }
        h2 {
          font-size: 1.5em;
        }
        header {
        position: relative;
      }
      #header-nav-menu-icon {
          position: absolute;
          right: 18px; top: 18px;
          display:block;
      }
      #header-nav ul, #header-nav:active ul {
        display: none;
        position: absolute;
          width:100%;
          right: 0px;
          top: 0;
        background: #354A5F;
      }
      #header-nav li {
        float: none;
        margin-left: 0;
        width: 100%;
        text-align: center;
      }
      #header-nav li a {
        display: inline-block;
          padding: 24px 0;
        width: 100%;
      }
      #header-nav li a:hover {
        background: #5D6E7F;
        text-decoration: none;
      }
      a#header-nav-button {
        padding: 0;
        border: none;
        border-radius: 0;
        background: transparent;
        font-weight: normal;
        color: #FFF;
      }
      #header-nav:hover ul {
        display: block;
      }
        #sec1{
          padding: 0px 32px;
        }
      #sec1-left {
        float: none;
        width: 100%;
      }
      #sec1-right {
        display: none;
      }
        .sec1-login {
          margin-top: 20px;
          width: 98%;
        }
        .sec1-login a {
          display:block;
          margin-right: 3px; padding: 10px 17px;
          text-align: center;
          white-space: nowrap;
          text-decoration: none;
          color: #0A6ED1;
        }
        .sec1-login a:hover {
          text-decoration: underline;
          color: #085CAF;
        }
        .sec1-login-button {
          padding: 10px 12px;
          border-radius: 4px;
          box-shadow: 0px 1px 0px 0px rgba(0, 0, 0, 0.06);
          background: #0A6ED1;
          font-family: '72bold', BentonSans, arial, sans-serif;
          color: #FFF !important;
        }
        .sec1-login-button:hover {
          background: #0854A1;
          text-decoration: none !important;
        }
        #sec2{
          padding: 50px 0px 0px 32px;
        }
        #sec2-zoomed-video-box {
          max-width: 92%;
          height: 220px;
        }
        #sec2-left {
          float: none;
          width: 100%;
        }
        #sec2-right {
          margin-left: 0; padding-top: 100px;
        }
        #sec3{
          padding: 42px 32px;
      }
        #sec3 h2 {
          width: 100%;
        }
        #floating-boxes {
          display: block;
        }
        #floating-boxes .box {
          padding: 0px 10px 0px;
          width: auto;
        }
        #sec4 {
          padding: 42px 32px;
        }
        #sec4-left {
          display:none;
      }
        #sec4-right {
          margin-left: 0px;
          padding: 0px 0px;
        max-width: 100%;
      }
        #sec6 h2 {
          font-size: 1.35em;
        }
        #sec6-contact-us h3 {
          font-size:1.35em;
        }
      #footer-nav {
        float: none;
        text-align: left;
          text-align: center;
      }
      #footer-nav li {
        float: none;
        margin: 10px 0px;
        width: 100%;
      }
    }
    :target:before {
      content: "";
      display: block;
      height: 64px; /* fixed header height*/
      margin: -64px 0 0; /* negative fixed header height */
    }

    .dropdown {
      position: relative;
      display: inline-block;
    }

    .dropdown-body {
        padding-top: 20px;
    }

    .dropdown-content {
      display: none;
      position: absolute;
      box-shadow: 0px 8px 16px 0px rgba(0,0,0,0.2);
      z-index: 1;
      min-width: max-content;
      border: none;
      border-radius: 4px;
    }

    .dropdown-content-nav a {
      padding: 12px 16px;
      text-decoration: none;
      display: block;
      color: #354A5F !important;
    }

    .dropdown-content-nav {
      top:28px;  /* 18 font + 10 padding,*/
      background-color: #f9f9f9;
      right:0;
    }

   .dropdown-content-body {
      top: 46px;
      background-color: #0A6ED1;
    }

    .dropdown-content-body a {
      padding: 12px 16px;
      text-decoration: none;
      display: block;
      color: #FFF !important;
    }

    .dropdown-content-body-dis {
        top: 46px;
        background: #CCC;
    }

    .dropdown-content-body-dis a {
        padding: 12px 16px;
        text-decoration: none;
        display: block;
        color: #FFF !important;
    }

    .dropdown-content-nav a:hover {background-color: #f1f1f1; border-radius: 4px;}
    .dropdown-content-body a:hover {background-color: #085CAF; border-radius: 4px;}
    .dropdown-content-body-dis a:hover {background-color: #CCC; border-radius: 4px;}
    .dropdown:hover .dropdown-content { display: block}
  </style>

</head>
<body>
<header>
  <div id="header-content">
    <div id="header-logo">
      <span class="sap4me"><%= translate('SHARED.SAP_C4HANA_COCKPIT') %></span>
    </div>
    <div id="header-nav">
      <a href="#header-content" id="header-nav-menu-icon"></a>
      <ul>
        <li><a href="#faq"><%= translate('SHARED.FAQ') %></a></li>
        <li>
          <% if (locals.flash.browserNotSupported || locals.flash.safariNotSupported) { %>
            <a id="header-nav-button-disabled" title="<%= translate('LANDING.SIGN_IN_DISABLED_TOOLTIP') %>">
          <% } else { %>
            <a href="/auth" id="header-nav-button">
          <% } %>
            <%= translate('SHARED.SIGN_IN') %></a>
        </li>
      </ul>
    </div>
  </div>
</header>

<!-- ----------------------------------------------------------------------- -->

<% if (locals.flash.hasMessage) { %>
  <div class="bg-gray" style="padding: 110px 0 55px 0;">
    <div style="text-align: center">
      <% if (locals.flash.sessionExpiredMessage) { %>
        <p class="slogan"><span class="sapc4cockpit"><%= translate('LANDING.SESSION_EXPIRED') %></span></p>
      <% } %>
        <% if (locals.flash.logoutMessage) { %>
          <p class="slogan"><span class="sapc4cockpit"><%= translate('LANDING.SIGNED_OUT') %></span></p>
           <iframe style="display: none" src="<%=locals.accountsLogoutURL%>"></iframe>
           <iframe style="display: none" src="<%=locals.ycloudLogoutURL%>"></iframe>
        <% } %>
      <% if (locals.flash.unableToFindAccount) { %>
        <p class="slogan"><span class="sapc4cockpit"><%= translate('LANDING.CANNOT_FIND_ACCOUNT') %></span></p>
      <% } %>
      <% if (locals.flash.unableToFindC4Account) { %>
        <p class="slogan"><span class="sapc4cockpit"><%= translate('LANDING.CANNOT_FIND_C4_ACCOUNT') %></span></p>
      <% } %>
      <% if (locals.flash.technicalErrorOccurred) { %>
        <p class="slogan"><span class="sapc4cockpit"><%= translate('LANDING.UNKNOWN_ERROR') %></span></p>
      <% } %>
      <% if (locals.flash.browserNotSupported) { %>
        <p class="slogan"><span class="sapc4cockpit"><%= translate('LANDING.BROWSER_NOT_SUPPORTED') %></span></p>
      <% } %>
      <% if (locals.flash.safariNotSupported) { %>
        <p class="slogan"><span class="sapc4cockpit"><%= translate('LANDING.SAFARI_NOT_SUPPORTED') %></span></p>
        <p class="slogan"><span class="sapc4cockpit"><%= translate('LANDING.RECOMMEND_CHROME') %></span></p>
      <% } %>
      <% if (locals.flash.errorCallingGetOidc) { %>
          <p class="slogan"><span class="sapc4cockpit"><%= translate('LANDING.GET_OIDC_CONFIG_ERROR') %></span></p>
      <% } %>
    </div>
  </div>
<% } %>

<!-- ----------------------------------------------------------------------- -->

<section class="bg-white" id="sec1">
  <div class="wrapper">
    <div id="sec1-left">
      <div id="sec1-intro">
        <p class="slogan"><%= translate('SHARED.SAP_C4HANA_COCKPIT') %></p>
        <h1><%= translate('LANDING.HOME_SAP_C4HANA') %></h1>
         <div class="sec1-login">
             <% if (locals.flash.browserNotSupported || locals.flash.safariNotSupported) { %>
                <a class="sec1-login-button-disabled" title="<%= translate('LANDING.SIGN_IN_DISABLED_TOOLTIP') %>">
             <% } else { %>
                 <a href="/auth" class="sec1-login-button">
              <% } %>
                 <%= translate('SHARED.SIGN_IN') %></a>
                 <a href="#faq3"><%= translate('LANDING.REQUEST_ACCESS') %></a>
                 <a href="<%= translate('LANDING.FORGOT_PASSWORD.HREF.URL') %>"><%= translate('LANDING.FORGOT_PASSWORD.TEXT') %></a>
         </div>
      </div>
    </div>
    <div id="sec1-right">
      <div id="sec1-zoomed-image-box">
        <div id="sec1-zoomed-image"></div>
      </div>
    </div>
  </div>
</section>

<!-- ----------------------------------------------------------------------- -->

<section class="bg-gray">
  <div id="sec3" class="letterbox">
    <h2><%= translate('LANDING.INFUSE_SAP_C4HANA') %></h2>
    <div id="floating-boxes">
      <div class="box">
        <div id="box1" class="imagebox" style="background: url('assets/images/box1.png') no-repeat 50% 0%;" data-rjs="2"></div>
        <h3><%= translate('LANDING.ENTITLEMENTS') %></h3>
        <p><%= translate('LANDING.ENTITLEMENTS_TEXT') %></p>
      </div>
      <div class="box">
        <div id="box2" class="imagebox" style="background: url('assets/images/box2.png') no-repeat 50% 0%;" data-rjs="2"></div>
        <h3><%= translate('LANDING.AUTHORIZATIONS') %></h3>
        <p><%= translate('LANDING.AUTHORIZATIONS_TEXT') %></p>
