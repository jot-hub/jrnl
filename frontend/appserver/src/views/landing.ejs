    }
      #floating-boxes .imagebox{
        margin:0 auto;
        width: 260px; height: 260px;
        background-size: contain !important;
    }
    /* -------------------------------------------------------------------------- */
      #sec4 {
      margin: 0px auto;
        overflow: hidden;
        max-width: 1400px;
    }
      #sec4-left {
      float: left;
        width: 50%;
        padding: 80px 0px 0px 0px;
      }
