      }
      #sec2-right {
        margin-left: 50%;
      }
      #sec2-intro {
        margin: 0px auto 0px 0px;
        padding: 0 10px 100px 0px;
        max-width: 620px;
      }
/* -------------------------------------------------------------------------- */
      #sec3 {
        z-index: 2;
        position:relative;
        padding: 100px 10px 0px 32px;
      text-align: center;
    }
      #sec3 h2 {
        margin: 0 auto 80px;
        width: 90%;
    }
      #floating-boxes {
      display: flex;
      justify-content: space-between;
      flex-wrap: wrap;
    }
      #floating-boxes h3 {
        margin-top: 42px; padding: 10px 0px 5px;
