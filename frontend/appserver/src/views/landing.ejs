    }
    /* -------------------------------------------------------------------------- */
      header {
      background: #354A5F;
      color: #FFF;
        position: fixed;
        z-index: 99;
        width:100%;
    }
    #header-content {
        margin: 0px auto; padding: 20px 32px;
      max-width: 1400px ;height: 24px;
    }
    #header-logo, #footer-tagline {
      background-image: url('data: image/png;base64,');
